# FilmAgent/.env.example

# Example environment variables for FilmAgent

# Credentials for logging into video generator websites
VIDEO_GEN_USERNAME=example_username
VIDEO_GEN_PASSWORD=example_password

# Directory for storing generated images
IMAGE_OUTPUT_DIR=./generated_images

# Path to the font file for image generation
FONT_PATH=/usr/share/fonts/truetype/dejavu/DejaVuSans-Bold.ttf

# Path to the script JSON file
SCRIPT_PATH=./script.json

# Time interval in seconds between generated images
IMAGE_INTERVAL=5
